{
  "name": "api-store",
  "private": true,
  "version": "1.0.0",
  "description": "Phantombuster's API store",
  "scripts": {
    "eslint": "eslint --max-warnings 60 'libs/**/*.js' 'store/**/*.js'",
    "eslint:error": "eslint --max-warnings 40 'libs/**/*.js' 'store/**/*.js' --quiet",
    "tslint": "tslint 'libs/**/*.ts' 'store/**/*.ts'",
    "build": "npm run tslint && tsc --diagnostics",
	"build:watch": "tsc --watch",
	"buildmd": "markdown-pp --watch ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phantombuster/api-store.git"
  },
  "author": "Phantombuster SAS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phantombuster/api-store/issues"
  },
  "homepage": "https://phantombuster.com/api-store",
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/puppeteer": "^1.11.1",
    "eslint": "^5.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/jquery": "^3.3.29",
    "phantombuster-sdk": "^0.2.1"
  }
}
